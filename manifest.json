{
    "name": "Cleo",
    "description": "Ai Powered Dictionnary",
    "version": "1.0",
    "manifest_version": 3,
    "content_scripts": [
      {
        "matches": ["http://*/*", "https://*/*"],
        "js": ["contentScript.js"]
      }],
    "action": {
      "default_popup": "src/popup/index.html"
    },
    "permissions": ["storage"]
  }